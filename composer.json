{
    "name": "sebastienheyd/boilerplate",
    "description": "Laravel Boilerplate based on AdminLTE 3 with user management, roles, permissions, logs viewer, ...",
    "keywords": ["laravel", "boilerplate", "backend", "adminlte", "laratrust", "laravelcollective", "active", "admin"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "arcanedev/log-viewer": "^5.0|^7.0|^8.0|^9.0|^10.0",
        "creativeorange/gravatar": "^1.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "intervention/image": "^2.5",
        "laravel/ui": "^1.2|^2.0|^3.0|^4.0",
        "lavary/laravel-menu": "^1.7",
        "matthiasmullie/minify": "^1.3",
        "nesbot/carbon": "^2.0",
        "pusher/pusher-php-server": "^6.0|^7.0",
        "santigarcor/laratrust": "^6.0|^7.0",
        "sebastienheyd/active": "^1.0",
        "spatie/laravel-html": "^2.0|^3.0",
        "yajra/laravel-datatables-oracle": "^9.0|^10.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.9|^3.0",
        "fakerphp/faker": "^1.20",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/php-code-coverage": "^7.0|^8.0|9.2.18",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "suggest": {
        "sebastienheyd/boilerplate-packager": "Package manager for sebastienheyd/boilerplate",
        "sebastienheyd/boilerplate-media-manager": "Media manager for sebastienheyd/boilerplate",
        "sebastienheyd/boilerplate-email-editor": "Visual editor for e-mails for sebastienheyd/boilerplate"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebastienheyd\\Boilerplate\\Tests\\": "tests/",
            "Sebastienheyd\\Boilerplate\\Database\\Factories\\": "database/factories/",
            "App\\": "testbench/app/"
        }
    },
    "authors": [
        {
            "name": "SÃ©bastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\Boilerplate\\BoilerplateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
